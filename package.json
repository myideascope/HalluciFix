{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest --run",
    "test:coverage": "vitest --run --coverage",
    "test:watch": "vitest --watch",
    "test:integration": "vitest --config vitest.integration.config.ts --run",
    "test:integration:watch": "vitest --config vitest.integration.config.ts",
    "test:integration:ui": "vitest --config vitest.integration.config.ts --ui",
    "test:integration:coverage": "vitest --config vitest.integration.config.ts --run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:mobile": "playwright test --config=playwright.mobile.config.ts",
    "test:e2e:mobile:ui": "playwright test --config=playwright.mobile.config.ts --ui",
    "test:e2e:mobile:headed": "playwright test --config=playwright.mobile.config.ts --headed",
    "test:e2e:mobile:report": "playwright show-report playwright-report-mobile",
    "test:all": "npm run test:run && npm run test:integration && npm run test:e2e",
    "coverage:check": "node scripts/coverage-check.js",
    "coverage:enforce": "npm run test:coverage && npm run coverage:check",
    "test:report": "node scripts/test-reporter.js",
    "test:report:verbose": "node scripts/test-reporter.js --verbose",
    "setup-env": "./scripts/setup-env.sh",
    "validate-env": "node scripts/validate-env.cjs",
    "validate-oauth": "node scripts/validate-oauth-config.js",
    "validate-oauth-integration": "node scripts/validate-oauth-integration.js",
    "test-oauth-flow": "node scripts/test-oauth-flow.js",
    "validate-oauth-security": "node scripts/validate-oauth-security.js",
    "oauth-summary": "node scripts/oauth-implementation-summary.js"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.908.0",
    "@google-cloud/storage": "^7.16.0",
    "@supabase/supabase-js": "^2.55.0",
    "google-auth-library": "^10.2.1",
    "googleapis": "^156.0.0",
    "lucide-react": "^0.344.0",
    "openai": "^6.3.0",
    "pdf-parse": "^1.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@faker-js/faker": "^10.0.0",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.7.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.8",
    "autoprefixer": "^10.4.18",
    "chokidar": "^4.0.3",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^27.0.0",
    "msw": "^2.11.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^2.1.8"
  }
}
