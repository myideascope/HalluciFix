{
  "name": "hallucifix-file-processor",
  "version": "1.0.0",
  "description": "Lambda function for processing files uploaded to S3",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "package": "npm run build && zip -r function.zip index.js node_modules/",
    "deploy": "aws lambda update-function-code --function-name hallucifix-file-processor --zip-file fileb://function.zip"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/client-dynamodb": "^3.450.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.126",
    "@types/node": "^20.8.0",
    "typescript": "^5.2.2"
  }
}